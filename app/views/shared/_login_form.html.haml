= form_for(:session, url: sessions_path) do |f|
  .row
    .block
      = f.text_field :username,  placeholder: 'Username'
  .row
    .block
      = f.password_field :password,  placeholder: 'Password'

  = f.submit "Submit", :class => "btn btn-primary btn-block"
  = link_to "Forgot Password?", "#"